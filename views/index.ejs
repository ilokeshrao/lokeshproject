<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Instrument+Serif&display=swap" rel="stylesheet">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Bruno+Ace+SC&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="/css/styles.css">

</head>

<body>
  <%- include('../partials/header.ejs')%>

    <section id="banner">
      <div class="container">
        <div class="row">
          <div class="col-md-8">
            <h4>
              <%=bannerRecord.title%>
            </h4>
            <p>
              <%=bannerRecord.desc%>
            </p>
            <a href="/banner"><button class="btn btn-success">more details...</button></a>
          </div>
          <div class="col-md-4">
            <img style="width: 400px;" src="/upload/<%=bannerRecord.img%>" class="img-fluid d-block " alt="">
          </div>
        </div>
      </div>
    </section>

    <section id="service">
      <div class="container">
        <div class="row ">
          <h3>service</h3>
          <%serviceRecord.forEach((result)=>{%>
            <div class="col-md-3">
              <img src="/upload/<%= result.img%>" class="d-block" alt="" width="100px">
              <h4>
                <%= result.title%>
              </h4>
              <p>
                <%=result.desc%>
              </p>
              <a href="/servicedetails/<%=result.id%>"><button class="btn btn-info">more details...</button></a> 
            </div>
            <%})%>


        </div>
      </div>
      </div>
    </section>
    <section id="testi">
      <div class="container">
        <div class="row">
          <h3>Testinominals</h3>

          <%testiRecord.forEach((result)=>{%>
            <div class="col-md-4">
              <div class="card" style="width: 18rem;">
                <img class="mx-auto" src="/upload/<%=result.img%>" alt="">
                <div class="card-body">
                  <p class="card-test"> <span><i class="bi bi-quote"></i></span> <%=result.quotes%> <span> <i
                        class="bi bi-quote"></i> </span>
                  </p>
                  <h5 class="card-title"><%=result.name%></h5>
                  <button class="mx-auto d-block btn btn-success form-control"><i class="bi bi-star"></i><i
                      class="bi bi-star"></i><i class="bi bi-star"></i><i class="bi bi-star"></i><i
                      class="bi bi-star"></i></button>
                </div>
              </div>
              </div>
              <%})%>


            </div>
        </div>
    </section>
    <section id="profile">
      <div class="container">
        <div class="row">
          <h2>profile</h2>
          <div class="col-md-12 d-flex">
            <div class="card" style="width: 10rem;">
              <img src="/media/business.png" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">Lokesh rao</h5>
                <p class="card-text">software engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.5.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">devendar rao</h5>
                <p class="card-text">software engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.4.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">sanjay rao</h5>
                <p class="card-text">app engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.4.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title"> mohit rao</h5>
                <p class="card-text">web engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.2.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">abhi rao</h5>
                <p class="card-text">software engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.4.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">anil rao</h5>
                <p class="card-text">software engineer</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <section id="footer">
      <div class="container">
        <div class="row">
          <div class="col-md-4"><img src="/media/logo.jpg" alt="" width="100px">
          <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Nesciunt voluptate molestias laborum facilis eligendi illum rem? Ullam enim sed odit? Nisi accusamus temporibus distinctio commodi necessitatibus officiis quae optio recusandae!</p>
          </div>
          <div class="col-md-4">
            <h2>Indigo Pvt Ltd</h2>
            <table>
              <tr>
                <td><i class="bi bi-buildings"></i></td>
                <td><p><%=addRecord.add%></p></td>
              </tr>
              <tr>
                <td><i class="bi bi-telephone"></i></td>
                <td><p>141-<%=addRecord.tel%></p></td>
              </tr>
              <tr>
                <td><i class="bi bi-phone"></i></td>
                <td><p><%=addRecord.mobile%></p></td>
              </tr>
              <tr>
                <td><i class="bi bi-envelope-heart"></i></td>
                <td><p><%=addRecord.email%> </p></td>
              </tr>
            </table>
  
          </div>
          <div class="col-md-4">
            <h2>Query form</h2>
            <form action="" method="post">
           <input type="email" name="email" id="" class="form-control" placeholder="email">
          <textarea name="query" id="" class="form-control mt-2 mb-2" placeholder="media-query"></textarea>
          <button class="btn btn-danger form-control">post query</button> 
          </form>
          </div>
        </div>
      </div>
      <div class="container text-center">
        <div class="row">
          <div class="col-md-12">
           <a href="<%=addRecord.insta%>" target="_blank"><img src="/media/instagram.png" alt=""></a>
            <img src="/media/facebook.png" alt="">
            <img src="/media/snapchat-removebg-preview3.png" alt="">
            <img src="/media/whatshap-removebg-preview2.png" alt="">
          </div>
        </div>
      </div>
    </section>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
</body>

</html>